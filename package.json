{
	"name": "robot-questions",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"start": "remix-serve ./build/server/index.js",
		"typecheck": "tsc",
		"routes:gen": "routes-gen -d @routes-gen/remix",
		"db:generate": "drizzle-kit generate --config=./drizzle.config.ts",
		"db:push": "drizzle-kit push:sqlite",
		"db:migrate": "tsx app/db/migrate.ts",
		"db:reset": "rm -f app/db/sqlite.db && npm run db:migrate"
	},
	"dependencies": {
		"@ai-sdk/deepseek": "^0.2.8",
		"@ai-sdk/groq": "^1.2.8",
		"@ai-sdk/openai": "^1.3.9",
		"@libsql/client": "^0.15.4",
		"@mastra/core": "^0.8.2",
		"@radix-ui/react-popover": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.0",
		"@radix-ui/react-toast": "^1.2.7",
		"@remix-run/node": "^2.16.4",
		"@remix-run/react": "^2.16.4",
		"@remix-run/serve": "^2.16.4",
		"@remix-validated-form/with-zod": "^2.0.7",
		"@routes-gen/remix": "^0.3.7",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.41.0",
		"isbot": "^4.1.0",
		"lucide-react": "^0.487.0",
		"motion": "^12.6.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-qr-code": "^2.0.15",
		"remix-validated-form": "^5.1.5",
		"routes-gen": "^0.6.1",
		"tailwind-merge": "^3.2.0",
		"tailwindcss-animate": "^1.0.7",
		"tw-animate-css": "^1.2.5",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@remix-run/dev": "^2.16.4",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"autoprefixer": "^10.4.19",
		"drizzle-kit": "^0.30.6",
		"eslint": "^8.38.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.4",
		"tsx": "^4.7.1",
		"typescript": "^5.1.6",
		"vite": "^6.0.0",
		"vite-tsconfig-paths": "^4.2.1"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
